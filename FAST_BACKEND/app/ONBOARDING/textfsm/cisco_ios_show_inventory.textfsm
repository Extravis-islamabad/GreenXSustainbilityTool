Value NAME (.*)
Value DESCR (.*)
Value PID (\S+)
Value VID (\S*)
Value SN (\S+)

Start
  ^NAME:\s+"${NAME}",\s+DESCR:\s+"${DESCR}" -> Continue
  ^PID:\s+${PID}\s*,\s+VID:\s+${VID}\s*,\s+SN:\s+${SN} -> Record
  ^PID:\s+${PID}\s*,\s+VID:\s+${VID}\s*,\s+SN:\s+ -> Record
  ^PID:\s+${PID}\s*,\s+VID:\s+\s*,\s+SN:\s+${SN} -> Record
  ^PID:\s+${PID}\s*,\s+VID:\s+${VID}.* -> Record
  ^PID:\s+${PID}\s*,.*SN:\s+${SN} -> Record
